{
    "Equation": {
        "prefix": "equation",
        "body": [
            "\\begin{equation}",
            "\t$0",
            "\t\\label{eq:$1}",
            "\\end{equation}"
        ],
        "description": "Add a Equation"
    },
    "Figure:Ref": {
        "prefix": "figure:ref",
        "body": ["Рисунок~\\ref{fig:$2}$0"],
        "description": "Ref for Figure"
    },
    "Listing:Ref": {
        "prefix": "listing:ref",
        "body": ["Листинг~\\ref{lst:$2}$0"],
        "description": "Listing"
    },
    "Section:Ref": {
        "prefix": "section:ref",
        "body": ["${1:Section}~\\ref{sec:$2}$0"],
        "description": "Section Reference"
    },
    "Chapter": {
        "prefix": "h0",
        "body": [
            "\\chapter{$1}\\label{chap:${1/([a-zA-Z]+)|([^a-zA-Z]+)/${1:/downcase}${2:+_}/g}} % (fold)",
            "${0:$TM_SELECTED_TEXT}",
            "% chapter $1 (end)"
        ],
        "description": "Chapter"
    },
    "Section": {
        "prefix": "h1",
        "body": [
            "\\section{$1}\\label{sec:${1/([a-zA-Z]+)|([^a-zA-Z]+)/${1:/downcase}${2:+_}/g}} % (fold)",
            "${0:$TM_SELECTED_TEXT}",
            "% section $1 (end)"
        ],
        "description": "Section"
    },
    "Sub Section": {
        "prefix": "h2",
        "body": [
            "\\subsection{$1}\\label{sub:${1/([a-zA-Z]+)|([^a-zA-Z]+)/${1:/downcase}${2:+_}/g}} % (fold)",
            "${0:$TM_SELECTED_TEXT}",
            "% subsection $1 (end)"
        ],
        "description": "Sub Section"
    },
    "Sub Sub Section": {
        "prefix": "h3",
        "body": [
            "\\subsubsection{${1:subsubsection name}}\\label{sec:${1/([a-zA-Z]+)|([^a-zA-Z]+)/${1:/downcase}${2:+_}/g}} % (fold)",
            "${0:$TM_SELECTED_TEXT}",
            "% subsubsection $1 (end)"
        ],
        "description": "Sub Sub Section"
    },
    "Paragraph": {
        "prefix": "h4",
        "body": [
            "\\paragraph{$1}\\label{par:${1/([a-zA-Z]+)|([^a-zA-Z]+)/${1:/downcase}${2:+_}/g}} % (fold)",
            "${0:$TM_SELECTED_TEXT}",
            "% paragraph $1 (end)"
        ],
        "description": "Sub Paragraph"
    },
    "Sub Paragraph": {
        "prefix": "h5",
        "body": [
            "\\subparagraph{$1}\\label{subp:${1/([a-zA-Z]+)|([^a-zA-Z]+)/${1:/downcase}${2:+_}/g}} % (fold)",
            "${0:$TM_SELECTED_TEXT}",
            "% subparagraph $1 (end)"
        ],
        "description": "Sub Paragraph"
    },
    "Table:Ref": {
        "prefix": "table:ref",
        "body": ["Таблица~\\ref{tab:$2}$0"],
        "description": "Table Reference"
    },
    "Figure": {
        "prefix": "figure",
        "body": [
            "\\begin{figure}",
            "\t\\begin{center}",
            "\t\t\\includegraphics[width=0.80\\textwidth]{figures/$1}",
            "\t\\end{center}",
            "\t\\caption{$2}\\label{fig:$2}",
            "\\end{figure}",
            "$0"
        ],
        "description": "Add a figure"
    },
    "Enumerate": {
        "prefix": "enumerate",
        "body": ["\\\\begin{enumerate}", "\t\\item $0", "\\\\end{enumerate}"],
        "description": "Add a enumerate"
    },
    "Cite": {
        "prefix": "cite",
        "body": ["\\cite{$1}$0"],
        "description": "Add a cite"
    }
}
